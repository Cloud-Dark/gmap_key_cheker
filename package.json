{
  "name": "gmaps-api-checker",
  "version": "1.0.0",
  "description": "Google Maps API Key Checker with Node.js proxy",
  "main": "backend/server.js",
  "bin": {
    "gmaps-api-checker": "backend/server.js"
  },
  "scripts": {
    "start": "node backend/server.js",
    "dev": "node backend/server.js",
    "build": "node build.js",
    "build:all": "pkg . --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --compress GZip",
    "build:win": "pkg . --targets node18-win-x64 --compress GZip",
    "build:linux": "pkg . --targets node18-linux-x64 --compress GZip",
    "build:mac": "pkg . --targets node18-macos-x64 --compress GZip"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "pkg": {
    "scripts": [
      "backend/server.js"
    ],
    "assets": [
      "index.html",
      "style.css",
      "script.js",
      "README.md",
      "node_modules/axios/**/*"
    ],
    "outputPath": "dist"
  },
  "author": "",
  "license": "MIT"
}
